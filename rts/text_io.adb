package body TEXT_IO is procedure PUT(C:Character)is procedure P(X:Integer);pragma IMPORT(C,P,"__text_io_put_char");begin P(Character'POS(C));end;procedure PUT(S:String)is procedure P(X:Integer);pragma IMPORT(C,P,"__text_io_put_char");begin for I in S'RANGE loop P(Character'POS(S(I)));end loop;end;procedure PUT_LINE(S:String)is begin PUT(S);NEW_LINE;end;procedure NEW_LINE is procedure N;pragma IMPORT(C,N,"__text_io_new_line");begin N;end;end TEXT_IO;
